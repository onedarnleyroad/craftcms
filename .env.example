# Read about configuration, here:
# https://craftcms.com/docs/4.x/config/
CRAFT_APP_ID=
CRAFT_ENVIRONMENT=dev
CRAFT_SECURITY_KEY=

# Database connection settings (config/db.php no longer required)
CRAFT_DB_DRIVER=mysql
CRAFT_DB_SERVER=127.0.0.1
CRAFT_DB_PORT=3306
CRAFT_DB_DATABASE=
CRAFT_DB_USER=root
CRAFT_DB_PASSWORD=
CRAFT_DB_SCHEMA=public
CRAFT_DB_TABLE_PREFIX=

# General settings (see config/general.php)
ALLOW_UPDATES=1
ALLOW_ADMIN_CHANGES=1
BACKUP_ON_UPDATE=1
CP_TRIGGER=admin
DEV_MODE=1
ENABLE_TEMPLATE_CACHING=0
ENVIRONMENT=local
HARD_MODE=1
IMAGE_DRIVER=imagick
IS_SYSTEM_LIVE=1
PRIMARY_SITE_URL=https://craftcms.ddev.site/
TEST_TO_EMAIL_ADDRESS=

# SMTP
SMTP_HOST=127.0.0.1
SMTP_PORT=1025
SMTP_USERNAME=
SMTP_PASSWORD=

# Plugins / Other
GOOGLE_ANALYTICS_ID=
IMAGER_TRANSFORMER=craft
KNOCK_KNOCK_IS_ENABLED=0
KNOCK_KNOCK_PASSWORD=
POSTMARK_TOKEN=
SEOMATIC_IS_LIVE=0
VITE_USE_DEV_SERVER=1
